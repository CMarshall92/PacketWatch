{
  "name": "packetwatch",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "clean": "turbo run clean",
    "dev": "npm run prisma:generate && turbo run dev",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "prisma:generate": "turbo run prisma:generate",
    "postinstall": "npm run prisma:generate"
  },
  "dependencies": {},
  "devDependencies": {
    "prettier": "^3.2.5",
    "turbo": "^2.5.6"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "packageManager": "npm@10.5.0",
  "workspaces": [
    "apps/clients/*",
    "apps/services/*",
    "packages/*"
  ],
  "overrides": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "@types/react": "18",
    "@types/react-dom": "18"
  }
}