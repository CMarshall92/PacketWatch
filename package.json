{
  "name": "packetwatch",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"turbo run dev\" \"turbo run dev:prisma --filter=@packetwatch/database\"",
    "dev:app": "turbo run dev",
    "build": "turbo run build",
    "prisma:generate": "turbo run prisma:generate",
    "clean:node": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "lint": "turbo run lint",
    "test": "turbo run test"
  },
  "dependencies": {},
  "devDependencies": {
    "concurrently": "^9.1.0",
    "prettier": "^3.2.5",
    "turbo": "^2.5.6"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "packageManager": "npm@10.5.0",
  "workspaces": [
    "apps/clients/*",
    "apps/services/*",
    "packages/*"
  ],
  "overrides": {
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "@types/react": "18",
    "@types/react-dom": "18"
  }
}
